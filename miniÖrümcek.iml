        "import requests\n",
        "from bs4 import BeautifulSoup\n",
        "\n",
        "def web_örücü(url):\n",
        "  \"\"\"\n",
        "  Belirli bir URL'den veri toplayan basit bir web örümceği.\n",
        "\n",
        "  Args:\n",
        "    url: Verilerin toplanacağı URL.\n",
        "\n",
        "  Returns:\n",
        "    Sayfanın başlığı ve tüm bağlantıların bir listesi.\n",
        "  \"\"\"\n",
        "  try:\n",
        "    response = requests.get(url)\n",
        "    response.raise_for_status()  # HTTP hatalarını kontrol et\n",
        "\n",
        "    soup = BeautifulSoup(response.content, \"html.parser\")\n",
        "\n",
        "    sayfa_başlığı = soup.title.string if soup.title else \"Başlık Bulunamadı\"\n",
        "    bağlantılar = [bağlantı.get(\"href\") for bağlantı in soup.find_all(\"a\")]\n",
        "\n",
        "    return sayfa_başlığı, bağlantılar\n",
        "\n",
        "  except requests.exceptions.RequestException as e:\n",
        "    print(f\"İstek hatası: {e}\")\n",
        "    return None, None\n",
        "\n",
        "# Kullanım örneği\n",
        "url = \"https://corbadatuzunolsun.org/\"\n",
        "başlık, bağlantılar = web_örücü(url)\n",
        "\n",
        "if başlık:\n",
        "  print(f\"Sayfa Başlığı: {başlık}\")\n",
        "  print(\"Bağlantılar:\")\n",
        "  for bağlantı in bağlantılar:\n",
        "    print(bağlantı)"
      ],
